{
  "presets": [
    "env",
    "flow",
    "react",
    "stage-1"
  ],
  "env": {
    "development": {
      "plugins": [
        ["transform-runtime", {
          "helpers": false,
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }]
      ]
    },
    "production": {
      "plugins": [
        ["transform-runtime", {
          "helpers": false,
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }],
        "transform-react-remove-prop-types"
      ]
    }
  }
}
