{
  "name": "react-redux-monorepo-starter-app-scripts",
  "version": "0.0.4",
  "private": true,
  "bin": {
    "scripts": "bin/scripts.js"
  },
  "scripts": {
    "start:dev": "cross-env APP_ENV=development npm run start",
    "start:staging": "cross-env APP_ENV=staging npm run start",
    "start:production": "cross-env APP_ENV=production npm run start",
    "start": "webpack-dev-server --config config/webpack.config.js",
    "build:dev": "cross-env APP_ENV=development npm run build",
    "build:staging": "cross-env APP_ENV=staging npm run build",
    "build:production": "cross-env APP_ENV=production npm run build",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "test": "jest --env=jsdom --config=../jest.config.js",
    "test:update": "jest --updateSnapshot --env=jsdom --config=../jest.config.js",
    "coverage": "jest --env=jsdom --config=../jest.config.js --coverage",
    "typecheck": "tsc",
    "typecheck:watch": "yarn typecheck -- --watch"
  },
